			$('hubbubImage').show();
			$('hubbubImage').down().src=$F('hubbubImage_'+$F('hubbubSelector'));
		} else {
			$('hubbubImage').hide();
		}
		new Ajax.Updater('subscription_output','/user/subscriptionoutput/'+viewUserString+'?hubbub_id='+$F('hubbubSelector'),{
			onComplete:function(){
				initialiseTabEvents();
			}
		});
	});
