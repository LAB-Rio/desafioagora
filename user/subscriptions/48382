	$('hubbubSelector').observe("change", function(event) {
		var viewUserString='';
		if ($('viewUser')!=null){
			viewUserString=$F('viewUser');
		}
		//$('crowdworksGroupSelect').select('.groupPic').each(function(element){ element.hide(); });
		if ($('hubbubImage_'+$F('hubbubSelector'))!=null){
			$('hubbubImage').show();
			$('hubbubImage').down().src=$F('hubbubImage_'+$F('hubbubSelector'));
		} else {
			$('hubbubImage').hide();
		}
		new Ajax.Updater('subscription_output','/user/subscriptionoutput/'+viewUserString+'?hubbub_id='+$F('hubbubSelector'),{
			onComplete:function(){
				initialiseTabEvents();
			}
		});
	});
</script>

