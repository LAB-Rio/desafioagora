        <script>
        (function() {
            var voteBoxes = document.getElementsByClassName('voteBox');
            for(var i = 0; i < voteBoxes.length; i++) {
                voteBoxes[i].onclick = function(e) {
                    e.preventDefault();
                }
            }
        })();
