                                                <div class="b-single-idea__comments-list-item-reply b-single-idea__comment-reply-3" id="comment-63730" >
                        
                                                
                        			                                    											
                                    
                       		           	                
           	 
                
                                
                	
                                            <div class="b-single-idea__comment-hide">
                                        
                            <div class="b-single-idea__comment-options">
                                                                 <i class="fa fa-angle-down b-single-idea__comment-options-trigger"></i>
                                 <div class="b-single-idea__comment-dropdown" style="display:none;">
                                     <div class="b-single-idea__comment-dropdown-inner">
                                     <ul>
                                         <li>
                                                                                     <a href="#" onclick="popUp('/report/form/63730',{'popUpTitle':'Report comment','width':400,'height':230,'showClose':0});return false;">Report this comment</a>
