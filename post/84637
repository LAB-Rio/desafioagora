            // Load the Visualization API and the piechart package.
            google.load('visualization', '1.0', {
              'packages':['corechart']
            });

            // Set a callback to run when the Google Visualization API is loaded.
            //google.setOnLoadCallback(drawChart);

            // Callback that creates and populates a data table,
            // instantiates the pie chart, passes in the data and
            // draws it.
            function drawChart(options) {

              // Create the data table.
              var overviewData = new google.visualization.DataTable();
              overviewData.addColumn('string', '');
              overviewData.addColumn('number', 'Event');
              overviewData.addRows(options.overview.rows);

              // Instantiate and draw our chart, passing in some options.
              var overviewChart = new google.visualization.BarChart(document.getElementById('overview_chart_div_' + options.id));
              overviewChart.draw(overviewData, options.overview);

              var linksData = new google.visualization.DataTable();
              linksData.addColumn('string', '');
              linksData.addColumn('number', 'Link');
              linksData.addRows(options.links.rows);

              var linksChart = new google.visualization.PieChart(document.getElementById('links_chart_div_' + options.id));
              linksChart.draw(linksData, options.links);
            }
        </script>
