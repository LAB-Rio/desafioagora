                data_track_addressbar: false,
                data_track_clickback:false
            }
    var addthis_share =
    {
       url: "https://desafioagorario.crowdicity.com/post/62152",
       title: "Aterramento dos Fios",
       description: " Olhar para o alto está cada vez mais feio ",
