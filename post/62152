<script>

	/*History.Adapter.bind(window,'statechange',function(e){ // Note: We are using statechange instead of popstate
	    var State = History.getState(); // Note: We are using History.getState() instead of event.state

	    
	    if(State.data.id) {
	    	var id = State.data.id;
	    } else {
	    	var id = State.hash.replace('/post/', '')
	    }
	
		
		jQuery.ajax('/post/view/' + id + '?postType=newDesign', {
			success: function(data) {
				jQuery('#idea_lowerFold').fadeOut(500, function() {
					jQuery('#idea_lowerFold').html(data);
					jQuery('#idea_lowerFold').fadeIn(500, function() {
						var prevPostId = jQuery('#prevPostData').data('id'),
							prevPostTitle = jQuery('#prevPostData').data('title'),
							nextPostId = jQuery('#nextPostData').data('id'),
							nextPostTitle = jQuery('#nextPostData').data('title');
					
						console.log(prevPostTitle);
						console.log(nextPostTitle);
						
						jQuery('#prevPostLink').data('id', nextPostId).data('title', nextPostTitle);
						jQuery('#nextPostLink').data('id', prevPostId).data('title', prevPostTitle);
						
						$('nextPostLink').addTip('test', ttoptions);
						
						Tips.list[0].content = prevPostTitle;
						Tips.list[1].content = nextPostTitle;
					});
				});
			}
		});
		
	});*/

</script>
        <div class="clear"><!-- IE --></div>
    </div>
</div>

