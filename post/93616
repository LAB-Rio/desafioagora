<script type="text/javascript">
    var addthis_config=
            {
                data_track_addressbar: false,
                data_track_clickback:false
            }
    var addthis_share =
    {
       url: "https://desafioagorario.crowdicity.com/post/93616",
       title: "Projeto Urbanístico FRANCISCO BICALHO",
       description: " Reurbanização do Canal do Mangue e Avenida Francisco Bicalho: - Plantação dos corredores de Palmeiras Imperiais, conforme registros históricos do local; - Construção de corredor BRT, incluindo estação em frente à estação Leopoldina, futuro terminal do Trem-Bala; - Eliminação (ou diminuição) dos canteiros que dividem as pistas, ganhando mais espaço para faixas de rolamento de automóveis; -...",
    }
    
        if (typeof addthis !== "undefined") {
            addthis.update('share', 'url', 'https://desafioagorario.crowdicity.com/post/93616');
            addthis.update('share', 'title', 'Projeto Urbanístico FRANCISCO BICALHO');
