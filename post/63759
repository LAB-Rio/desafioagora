                                                <div class="mix b-single-idea__comments-list-item" id="comment-63875" data-posted="2014-09-18 13:01:53" data-votes="0">
                        
                                                
                        			                                    											
                                    
                       		           	                
           	           			                                
                                
                                                                                
                                            
                                                                                        <div class="b-single-idea__comment-options">
                                                                                                 <i class="fa fa-angle-down b-single-idea__comment-options-trigger"></i>
                                                 <div class="b-single-idea__comment-dropdown" style="display:none;">
                                                     <div class="b-single-idea__comment-dropdown-inner">
                                                     <ul>
                                                         <li>
                                                                                                                  	<a href="#" onclick="popUp('/report/form/63875',{'popUpTitle':'Report comment','width':400,'height':230,'showClose':0});return false;">Report this comment</a>
                                                                                                                  </li>
                                                                                                              </ul>
