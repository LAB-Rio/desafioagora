            jQuery(".b-single-idea__comment-expand").on("click", function(e) {
                //console.log("expand comments");
                e.preventDefault();
                var parent = jQuery(this).attr("data-parent");
                jQuery("#comment-" + parent + " .b-single-idea__comment-hide").slideDown();
                jQuery("#b-single-idea__comment-expand-" + parent).slideUp();

            });

		    jQuery(".b-single-idea__comments-menu a").on('click', function(e){
