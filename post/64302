<script type="text/javascript">
    var addthis_config=
            {
                data_track_addressbar: false,
                data_track_clickback:false
            }
    var addthis_share =
    {
       url: "https://desafioagorario.crowdicity.com/post/64302",
       title: "Operações Urbanas na zona norte.",
       description: " Operações urbanas semelhantes a do Porto Maravilha em bairros da zona norte, principalmente ao longo das linhas férreas. ",
    }
    
        if (typeof addthis !== "undefined") {
            addthis.update('share', 'url', 'https://desafioagorario.crowdicity.com/post/64302');
            addthis.update('share', 'title', 'Operações Urbanas na zona norte.');
            addthis.update('share', 'description', ' Operações urbanas semelhantes a do Porto Maravilha em bairros da zona norte, principalmente ao longo das linhas férreas. ');
        }
    
</script>
    
