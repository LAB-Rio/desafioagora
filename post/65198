							prevPostTitle = jQuery('#prevPostData').data('title'),
							nextPostId = jQuery('#nextPostData').data('id'),
							nextPostTitle = jQuery('#nextPostData').data('title');
					
						console.log(prevPostTitle);
						console.log(nextPostTitle);
						
						jQuery('#prevPostLink').data('id', nextPostId).data('title', nextPostTitle);
						jQuery('#nextPostLink').data('id', prevPostId).data('title', prevPostTitle);
						
						$('nextPostLink').addTip('test', ttoptions);
						
						Tips.list[0].content = prevPostTitle;
						Tips.list[1].content = nextPostTitle;
					});
				});
			}
		});
		
	});*/

</script>
        <div class="clear"><!-- IE --></div>
    </div>
</div>

