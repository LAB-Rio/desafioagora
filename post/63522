            Voting is not open in this phase
        	</div>

<script>
	jQuery('.star-voting-trigger').each(function() {
		jQuery(this).hover(function() {
			var order = jQuery(this).data('order'),
				parent = jQuery(this).parent();
			
			parent.find('span').each(function(i) {
				if(i < order) {
					jQuery(this).addClass('star-active');
				}
			});
		}, function() {
			var order = jQuery(this).data('order'),
				parent = jQuery(this).parent();
			
			parent.find('span').each(function(i) {
				if(i < order && !jQuery(this).data('force-active')) {
					jQuery(this).removeClass('star-active');
				}
			});
		});
	});
</script>
