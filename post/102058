		<script>
            jQuery(function() {
			var userName = '';
			var cache = [];
			
			jQuery('#user_tag_input').keyup(function(e) {

				var key = 'which' in e ? e.which : e.keyCode;

				userName = jQuery('#user_tag_input').val();
				
				if(cache[Comment.hashCode(userName)]) {
					jQuery('#user_tag_results').html(cache[userName]).show();
				} else {
					if(userName.length > 2) {
						jQuery.ajax('/user/solr_search', {
							data: { q: userName, type: 'userTag' },
							dataType: 'json',
							success: function(data) {
								cache[Comment.hashCode(userName)] = data.tpl;
								
								if(data.tpl == 'No users found') {
									jQuery('#user_tag_results').html('').hide();
								} else {	
									jQuery('#user_tag_results').html(data.tpl).show();
									
									// bind "click" events to the newly created rows
									jQuery('.b-single-idea__user-tag-result').each(function() {
										jQuery(this).click(function() {
											
											var userId = jQuery(this).data('id');
											
											if(jQuery(this).hasClass('user-tag-selected')) {
												jQuery('#comment_userTags').val(jQuery('#comment_userTags').val().replace(userId, ''));
												jQuery(this).remove();
											} else {
												var userScreenname = jQuery(this).data('screenname');
												
												jQuery(this).addClass('user-tag-selected', 1);
												
												jQuery('.b-single-idea__new-comment-area-bottom div.clear').each(function() {
													jQuery(this).remove();
												});
												
												var clear = document.createElement('div');
												jQuery(clear).addClass('clear');
												
												jQuery('#comment_userTags').val(jQuery('#comment_userTags').val() + ',' + userId);
												jQuery('#comment_userScreennames').val(jQuery('#comment_userScreennames').val() + ',' + userScreenname);
												jQuery('.b-single-idea__new-comment-area-bottom').append(this);
												jQuery('.b-single-idea__new-comment-area-bottom').append(clear);
												jQuery('#user_tag_input').val('');
												jQuery('#user_tag_results').html('').hide();
											}
										});
									});	
								}
							}
						});
					}	
				}
			});
			
            jQuery(function(){
                // On first load, we let the comments be hidden, as they should,
                // to prevent the wierd gliding effect as comments fly into
                // position (if the layout changes from late-loading images)
                jQuery(".b-single-idea__comments-list-item").addClass('mixHide');
                Comment.resetCommentList();
            });


			jQuery('#commentForm').submit(function(e) {
				e.preventDefault();
				jQuery(this).find('.b-single-idea__new-comment-button').hide();
				var ideaId = jQuery('#idea_id').val();
				
				jQuery.ajax('/post/addcomment/102058', {
					data: jQuery('#commentForm').serialize(),
					type: 'post',
					success: function(data) {
                        if (data.success == 1) {
                            var newCommentId = data.post_id;
                            jQuery('.b-single-idea__new-comment-button').show();
                            Comment.commentsReload(ideaId, 0, newCommentId, newCommentId);
                        } else {
                            var message="";
                            var errorList="";
                            if (data.errors!=undefined){
                                var errors=data.errors;
                                for (var key in errors) {
                                    if (errors.hasOwnProperty(key)) {
                                        //alert(key + " -> " + p[key]);
                                        errorList=errorList+errors[key]+". ";
                                    }
                                }
                                message=errorList;
                            } else {
                                message = data.message;
                            }
                            jQuery('.b-single-idea__new-comment-button').show();
                            alert(message);
                        }
					}
				});
				
				return false;
			});
        });
		
		</script>

		</div>
	</div>
	
