this.lastThreadRefresh=Math.round(new Date().getTime()/1000);this.autoThreadRefresh=function(){if($F('message_parent')!=0){var path='/states/conversation/'+$F('message_key')+'/lastupdate.txt';new Ajax.Request(path,{onComplete:function(response){var lastUpdateTime=parseInt(response.responseText);if(lastUpdateTime>self.lastThreadRefresh){self.lastThreadRefresh=Math.round(new Date().getTime()/1000);var lastId=0;if($('message_lastId')!==null){lastId=$F('message_lastId');}
