this.initTimeline=function(currentPhase,phaseTimeLineContainerElementName){if(phaseTimeLineContainerElementName==undefined){phaseTimeLineContainerElementName='phaseTimeLineContainer';}
var connectorLengths=0;var phaseTimeLineContainerElement=$(phaseTimeLineContainerElementName)
phaseTimeLineContainerElement.select('.phaseConnector').each(function(element){l=element.getLayout();connectorLengths+=l.get('width');});var phaseTimeLineContainerElementLayout=phaseTimeLineContainerElement.getLayout();var fullWidth=phaseTimeLineContainerElementLayout.get('width');Phase.phaseBlockElements=phaseTimeLineContainerElement.select('.phase');Phase.phaseBlockCount=Phase.phaseBlockElements.length;var margins;if(Phase.phaseBlockElements[0]!=undefined){var wasCurrent=false;if(Phase.phaseBlockElements[0].hasClassName('current')){Phase.phaseBlockElements[0].removeClassName('current');}
margins=parseInt(Phase.phaseBlockElements[0].getStyle('marginLeft'))
+parseInt(Phase.phaseBlockElements[0].getStyle('marginRight'));if(wasCurrent){Phase.phaseBlockElements[0].addClassName('current');}}else{margins=7;}
var availableWidth=(fullWidth-connectorLengths)-margins;blockWidth=(availableWidth/Phase.phaseBlockCount)-margins;for(var i=0;i<Phase.phaseBlockCount;i++){Phase.phaseBlockElements[i].setStyle({width:blockWidth+'px'});}
Phase.updatePhaseBar(true,phaseTimeLineContainerElementName);if(currentPhase!==undefined){Phase.selectCategoryPhase(currentPhase);}
currentPhaseElement=Phase.getCurrentPhaseElement();if((currentPhaseElement!=undefined)&&($("numdays")!=undefined)){phaseId=currentPhaseElement.id.split('_')[1];var currentEndingTime=parseInt($F('phase_'+phaseId+'_enddateStamp'));var curTime=new Date().getTime();var secondsRemaining=(currentEndingTime*1000)-curTime;var secondsRemaining=Math.floor(secondsRemaining/1000);var theDaysBox=$("numdays");var theHoursBox=$("numhours");var theMinsBox=$("nummins");var theSecsBox=$("numsecs");daysLeft=Math.floor(secondsRemaining/86400);secondsRemaining=secondsRemaining-(daysLeft*86400);hoursLeft=Math.floor(secondsRemaining/3600);secondsRemaining=secondsRemaining-(hoursLeft*3600);minutesLeft=Math.floor(secondsRemaining/60);secondsRemaining=secondsRemaining-(minutesLeft*60);secondsLeft=secondsRemaining;theDaysBox.update(daysLeft);theHoursBox.update(hoursLeft);theMinsBox.update(minutesLeft);theSecsBox.update(secondsLeft);if(daysLeft<1){theDaysBox.addClassName('clock-item-inactive');}
if(hoursLeft<1){theHoursBox.addClassName('clock-item-inactive');}
if(minutesLeft<1){theMinsBox.addClassName('clock-item-inactive');}
if(secondsLeft<1){theSecsBox.addClassName('clock-item-inactive');}
Phase.clockRefresh=setInterval(function(){Phase.updatePhaseBar();var currentSeconds=theSecsBox.innerHTML;var currentMins=theMinsBox.innerHTML;var currentHours=theHoursBox.innerHTML;var currentDays=theDaysBox.innerHTML;if(currentSeconds==0&&currentMins==0&&currentHours==0&&currentDays==0){Phase.phaseExpire();}else if(currentSeconds==0&&currentMins==0&&currentHours==0){theDaysBox.update(currentDays-1);theHoursBox.update("23");theMinsBox.update("59");theSecsBox.update("59");}else if(currentSeconds==0&&currentMins==0){theHoursBox.update(currentHours-1);theMinsBox.update("59");theSecsBox.update("59");}else if(currentSeconds==0){theMinsBox.update(currentMins-1);theSecsBox.update("59");}else{theSecsBox.update(currentSeconds-1);}},1000);}}
this.updatePhaseBar=function(firstUpdate,phaseTimeLineContainerElementName){var d=new Date();var n=d.getMilliseconds();if(phaseTimeLineContainerElementName==undefined){phaseTimeLineContainerElementName='phaseTimeLineContainer';}
currentPhaseElement=Phase.getActivePhaseElement(phaseTimeLineContainerElementName);var currentPhaseId=currentPhaseElement.id.split('_')[1];var currentPhaseDuration=$F('phase_'+currentPhaseId+'_duration');var allPhaseDurations=[];var currentPhasePosition=-1;var pixelWidthUpToCurrent=25;var totalTimeLength=0;var startTimeOfCurrent=0;var endTimeOfCurrent=0;var widthOfCurrent=0;for(var i=0;i<Phase.phaseBlockCount;i++){phaseId=Phase.phaseBlockElements[i].id.split('_')[1];allPhaseDurations[i]=$F('phase_'+phaseId+'_duration');phaseTimeBarElement=Phase.phaseBlockElements[i].down('.phaseTimeBar');if(currentPhaseId==phaseId){currentPhasePosition=i;startTimeOfCurrent=parseInt($F('phase_'+phaseId+'_startdateStamp'));endTimeOfCurrent=parseInt($F('phase_'+phaseId+'_enddateStamp'));style_widthOfCurrent=Phase.phaseBlockElements[i].getStyle('width');widthOfCurrent=parseFloat(style_widthOfCurrent.substring(0,style_widthOfCurrent.length-2));totalTimeLength=endTimeOfCurrent-startTimeOfCurrent;curTime=new Date().getTime();timeThrough=(Math.floor(curTime/1000))-startTimeOfCurrent;if(firstUpdate){phaseTimeBarElement.show();phaseTimeBarElement.setStyle({width:'1px'});new Effect.Morph(phaseTimeBarElement,{duration:0.5,style:{width:((timeThrough/totalTimeLength)*widthOfCurrent)+'px'},queue:{position:'end',scope:'phaseUnique'+n}});}else{phaseTimeBarElement.show();phaseTimeBarElement.setStyle({width:((timeThrough/totalTimeLength)*widthOfCurrent)+'px'});}}else if(currentPhasePosition==-1){pixelWidthUpToCurrent+=Phase.phaseBlockElements[i].getWidth()+25;if(firstUpdate){phaseTimeBarElement.show();phaseTimeBarElement.setStyle({width:'0%'});new Effect.Morph(phaseTimeBarElement,{duration:0.5,style:{width:'100%'},queue:{position:'end',scope:'phaseUnique'+n},transition:Effect.Transitions.linear});}}}}
