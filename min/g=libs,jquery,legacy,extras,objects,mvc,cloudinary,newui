var Move={copy:function(e,target){var eId=$(e);var copyE=eId.cloneNode(true);var cLength=copyE.childNodes.length-1;copyE.id=e+'-copy';for(var i=0;cLength>=i;i++){if(copyE.childNodes[i].id){var cNode=copyE.childNodes[i];var firstId=cNode.id;cNode.id=firstId+'-copy';}}
$(target).appendChild(copyE);},element:function(e,target,type){var eId=$(e);if(type=='move'){$(target).appendChild(eId);}
else if(type=='copy'){this.copy(e,target);}}}
toggleBlind=function(elementName,toggleElement){if($(elementName).style.display=="none"){Effect.BlindDown($(elementName),{duration:0.25,queue:{position:'end',scope:'catScope'}});Effect.Appear($(elementName),{duration:0.25,from:0,to:1,queue:{position:'end',scope:'catFadeScope'}});if(($(toggleElement+":show")!=null)&&($(toggleElement+":hide")!=null)){$(toggleElement+":show").hide();$(toggleElement+":hide").show();}}else{Effect.BlindUp($(elementName),{duration:0.25,queue:{position:'end',scope:'catScope'}});Effect.Fade($(elementName),{duration:0.25,from:1,to:0,queue:{position:'end',scope:'catFadeScope'}});if(($(toggleElement+":show")!=null)&&($(toggleElement+":hide")!=null)){$(toggleElement+":show").show();$(toggleElement+":hide").hide();}}}
function selectTab(element){element=$(element);var parentElement=$(element).up().up().up();var tabElements=$$('#'+parentElement.id+' li');if(!$(element).up().hasClassName('disabled')){for(var i=0;i<tabElements.length;i++){tabElements[i].removeClassName('selected');if($("view"+tabElements[i].down().id)!=null){$("view"+tabElements[i].down().id).hide();}}
element.up().addClassName('selected');if($("view"+element.id)!=null){$("view"+element.id).show();}
var tabGroupElement=element.up().up().up();if($(tabGroupElement.id+"_currentTab")!=null){$(tabGroupElement.id+"_currentTab").setValue(element.id.substring(4));}
if(element.up().hasAttribute('data-tabaction')){var action=element.up().readAttribute('data-tabaction');element.up().removeAttribute('data-tabaction');$("view"+element.id).update("Loading...");new Ajax.Updater($("view"+element.id),action,{onCreate:function(){document.fire("crowdworks:updateStart",{'element':$("view"+element.id)});},onComplete:function(){document.fire("crowdworks:updateComplete",{'element':$("view"+element.id)});}});}}}
function disableTab(element){if(!$(element).up().hasClassName('disabled')){$(element).up().addClassName('disabled');}}
function enableTab(element){$(element).up().removeClassName('disabled');}
function getSelectedTabId(parentElement){var tabElements=$$('#'+parentElement.id+' li.selected');if(tabElements.length>0){return tabElements[0].down();}else{return false;}}
function selectNextTab(parentElement){var tabElements=$$('#'+$(parentElement).id+' li');var nextTab=null;for(var i=0;i<tabElements.length;i++){if(tabElements[i].hasClassName('selected')){if(i+1<tabElements.length){nextTab=tabElements[i+1].down();}else{nextTab=tabElements[0].down();}}}
selectTab(nextTab);}
function selectPreviousTab(parentElement){var tabElements=$$('#'+$(parentElement).id+' li');var previousTab=null;for(var i=0;i<tabElements.length;i++){if(tabElements[i].hasClassName('selected')){if(i-1>=0){previousTab=tabElements[i-1].down();}else{previousTab=tabElements[tabElements.length-1].down();}}}
selectTab(previousTab);}
var loadingIndicators=new Array();var loadingIndicator=function(element,useBox){this.targetElement=$(element);this.element=undefined;this.useBox=useBox;this.deactivate=function(){this.delayedExecutor.stop();if($(this.element)!=null){this.element.remove();}}
this.activate=function(pe){pe.stop();this.element=new Element('div');var parentWidth=this.targetElement.getWidth();this.element.setStyle({display:'none',zIndex:1000,position:'absolute'});$('docBody').insert({bottom:this.element});this.element.clonePosition(this.targetElement,{setWidth:false,setHeight:false,offsetTop:40,offsetLeft:(parentWidth/2)-(100/2)});var boxOutput='<h4 style="margin-bottom:2px;padding:0px;">Loading...</h4><img src="'+httpMode+'://resources.'+serverName+'/templates/common/images/hubbub_loader.gif" />';if(this.useBox==true){boxOutput='<div class="GB_popupLoader" style="z-index:1000;">'+boxOutput+'</div>';}
this.element.update(boxOutput);this.element.setStyle({width:'100px'});this.element.appear({duration:0.5,queue:{position:'end',scope:'nscope'}});}}
function addLoadingIndicator(element,useBox){if(useBox==undefined){useBox=true;}
var dontDoIt=false;for(var i=0;i<loadingIndicators.length;i++){if(loadingIndicators[i].targetElement.id==element.id){dontDoIt=true;}}
if(dontDoIt==false){var nli=new loadingIndicator(element,useBox)
nli.delayedExecutor=new PeriodicalExecuter(function(pe){nli.activate(pe);},0.5);loadingIndicators.push(nli);}}
function removeLoadingIndicator(element){var removeMe;for(var i=0;i<loadingIndicators.length;i++){if(loadingIndicators[i].targetElement.id==element.id){removeMe=loadingIndicators[i];}}
if(removeMe!=undefined){removeMe.deactivate();loadingIndicators=loadingIndicators.without(removeMe);}}
