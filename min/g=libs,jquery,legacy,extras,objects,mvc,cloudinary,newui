if(!dropdown_valid){errors.push(item);}}else if(type==="Textbox"){var textbox_valid=false;var input=$(item).select('input')[0];if(input.value){textbox_valid=true;}
if(!textbox_valid){errors.push(item);}}});if(errors.length>0){return false;}else{return true;}};this.checkMin=function(collection){var errors=[];collection.each(function(item){var min=$(item).readAttribute('data-question-min');var boxes=$(item).select('input[type=checkbox]');var checked=[];boxes.each(function(box){if(box.checked){checked.push(box);}});if(checked.length<min){errors.push(item);}});if(errors.length>0){return false;}else{return true;}};this.checkMax=function(collection){var errors=[];collection.each(function(item){var max=$(item).readAttribute('data-question-max');var boxes=$(item).select('input[type=checkbox]');var checked=[];boxes.each(function(box){if(box.checked){checked.push(box);}});if(checked.length>max){errors.push(item);}});if(errors.length>0){return false;}else{return true;}};this.saveDraft=function(el){var questionDivs=[];for(var i=1;i<=SurveyApp.current_page;i++){var tmpDivs=$('page-holder-'+i).select('[data-question]');tmpDivs.each(function(item){questionDivs.push(item);});}
