while(oe=newOverlayElements.shift()){activeOverlayElements.push(oe);}}}
var textBox_addDefaultText=function(element,text){element=$(element);element.writeAttribute('gb_defaultText',text);if(element!=null){if(element.getValue()==''){element.addClassName('inputBox_blank');element.setValue(element.readAttribute('gb_defaultText'));}
element.observe('focus',function(event){if(element.getValue()==element.readAttribute('gb_defaultText')){element.setValue('');}
element.removeClassName('inputBox_blank');});element.observe('click',function(event){if(element.getValue()==element.readAttribute('gb_defaultText')){element.setValue('');}
element.removeClassName('inputBox_blank');});element.observe('disco:fakeClick',function(event){if(element.getValue()==element.readAttribute('gb_defaultText')){element.setValue('');}
element.removeClassName('inputBox_blank');});element.observe('keydown',function(event){if(element.getValue()==element.readAttribute('gb_defaultText')){element.setValue('');}
element.removeClassName('inputBox_blank');});element.observe('blur',function(event){if(element.getValue()==''){element.setValue(element.readAttribute('gb_defaultText'));element.addClassName('inputBox_blank');}});var formElement=element.up('form');if(formElement!=null){formElement.observe('submit',function(event){if(element.getValue()==element.readAttribute('gb_defaultText')){element.setValue('');element.removeClassName('inputBox_blank');}});formElement.observe('disco:submit',function(event){if(element.getValue()==element.readAttribute('gb_defaultText')){element.setValue('');element.removeClassName('inputBox_blank');}});formElement.observe('disco:postSubmit',function(event){if(element.getValue()==''){element.setValue(element.readAttribute('gb_defaultText'));element.addClassName('inputBox_blank');}});}
return true;}else{return false;}}
var textBox_changeDefaultText=function(element,text){element=$(element);var updateText=false;if($F(element)==element.readAttribute('gb_defaultText')){updateText=true;}
element.writeAttribute('gb_defaultText',text);if(updateText){element.setValue(text);}}
