var initialiseTabEvents=function(){var i;var j;var innerTabs;var parentId;var tabContainers=$$('.crowdworksTabs');for(i=0;i<tabContainers.length;i++){if(!tabContainers[i].hasClassName('crowdworksTabsProcessed')){var totalWidth=0;parentId=tabContainers[i].id;innerTabs=$$('#'+parentId+' a[id^="tab_"]');for(j=0;j<innerTabs.length;j++){innerTabComtainer=innerTabs[j].up('li');if(!tabContainers[i].hasClassName('crowdworksTabsManual')){innerTabs[j].stopObserving('click',doTabEvent);innerTabs[j].observe('click',doTabEvent);if(innerTabComtainer.hasClassName('selected')){selectTab(innerTabs[j]);}}
var layout=innerTabComtainer.getLayout();totalWidth+=(layout.get('border-box-width'));}
var tabContainerUl=tabContainers[i].down('ul');var tabContainerWidth=tabContainers[i].getWidth();if(tabContainers[i].hasClassName('crowdworksTabsMobileScroll')){totalWidth+=30;}
if(tabContainerWidth<totalWidth){tabContainerUl.setStyle({'width':(totalWidth+1)+'px'});if(!tabContainers[i].hasClassName('crowdworksTabsMobileScroll')){tabContainerUl.writeAttribute('tabs_maxwidth',totalWidth+1);tabContainerUl.writeAttribute('tabs_containerwidth',tabContainerWidth);tabContainerUl.writeAttribute('tabs_offset',0);tabContainerUl.insert({after:new Element('div',{'class':'leftArrow'}).hide().observe('click',function(e){doTabScroll(e,1);})});tabContainerUl.insert({after:new Element('div',{'class':'rightArrow'}).observe('click',function(e){doTabScroll(e,-1);})});}}else{if(tabContainers[i].getWidth()!=0){tabContainerUl.setStyle({'width':'100%'});}}
tabContainers[i].addClassName('crowdworksTabsProcessed');}}}
var doTabScroll=function(e,distance){element=e.findElement().previous('ul');distance=distance*parseInt(element.readAttribute('tabs_containerwidth'));parentElement=element.up();leftOffset=element.positionedOffset()[0];minOffset=0-(element.readAttribute('tabs_maxwidth')-element.readAttribute('tabs_containerwidth'));newLeftOffset=leftOffset+distance;if(newLeftOffset>0){newLeftOffset=0;parentElement.select('.leftArrow')[0].hide();parentElement.select('.rightArrow')[0].show();}else if(newLeftOffset<minOffset){newLeftOffset=minOffset;parentElement.select('.leftArrow')[0].show();parentElement.select('.rightArrow')[0].hide();}else{parentElement.select('.rightArrow')[0].show();parentElement.select('.leftArrow')[0].show();}
element.morph('left:'+newLeftOffset+'px;',{duration:0.2});}
var doTabEvent=function(event){var parentElement=$(this).up().up().up();if(!parentElement.hasClassName('unclickableTabs')){selectTab(this);}
event.stop();}
var initialiseFormElements=function(){var i;var innerElementId;var ddb=$$('.GB_dropDownButton');for(i=0;i<ddb.length;i++){ddb[i].stopObserving('click',GB_doDropDownEvent);ddb[i].observe('click',GB_doDropDownEvent);}
var clf=$$('.GB_characterLimit');var mceEditor;for(i=0;i<clf.length;i++){GB_removeCharacterLimit(clf[i]);clf[i].observe('keypress',GB_characterLimitCheck);clf[i].observe('keyup',GB_characterLimitUpdate);GB_characterLimitUpdateOnElement(clf[i]);}}
var GB_removeCharacterLimit=function(element){element.stopObserving('keypress',GB_characterLimitCheck);element.stopObserving('keyup',GB_characterLimitCheck);}
var GB_characterLimitCheck=function(event){var element=event.element();var KeyID;if(typeof(event.memo.keyCode!==undefined)){KeyID=event.memo.keyCode;}else{KeyID=event.keyCode;}
if(!GB_characterLimitCheckFunc(element,KeyID)){event.stop();}}
var GB_characterLimitCheckFunc=function(element,KeyID){var allow=true;var maxChars=element.readAttribute('data-gbcharacterlimit');var mceEditor=tinyMCE.get(element.id);if(mceEditor){element.setValue(mceEditor.getContent({format:'text'}));}
if(GB_characterLimitGetLength($F(element))>=parseInt(maxChars)&&(KeyID!=8)&&(KeyID!=46)){allow=false;}
GB_characterLimitUpdateOnElement(element);return allow;}
var GB_characterLimitUpdate=function(event){var element=event.element();GB_characterLimitUpdateOnElement(element);}
var GB_characterLimitUpdateOnElement=function(element,mceEditor){var maxChars=element.readAttribute('data-gbcharacterlimit');if(typeof mceEditor==='undefined'){mceEditor=tinyMCE.get(element.id);}
if(mceEditor){element.setValue(mceEditor.getContent({format:'text'}));}
var countElement=$(element.id+'_characterCount');if(countElement!=null){$(element.id+'_characterCount').update(parseInt(maxChars)-GB_characterLimitGetLength($F(element)));}}
var GB_characterLimitGetLength=function(str){str=str.replace(/\r/g,'').replace(/\n/g,'');return str.length;}
var GB_doDropDownEvent=function(event){var continueEvent=true;if(continueEvent){GB_doDropDown(this,event);event.preventDefault();}}
var GB_doDropDown=function(element,event){element=$(element);var elementBits=element.id.split(':');innerElementId="GB_dropDownArea_inner:"+elementBits[1];if($('GB_dropDownButton:'+elementBits[1])!=null){element=$('GB_dropDownButton:'+elementBits[1]);}
if(element.hasClassName('GB_dropDownButton')){if(element.hasClassName('GB_dropDownButtonSelected')){element.removeClassName('GB_dropDownButtonSelected');$(innerElementId).hide();}else if(!element.hasClassName('GB_dropDownButtonDisabled')){element.addClassName('GB_dropDownButtonSelected');$(innerElementId).show();newOverlayElements.push($(innerElementId));}}else{if(event!=undefined){if(element.hasClassName('GB_dropDownPersist')){clickElement=event.findElement('#'+element.id);if(clickElement==document||clickElement==undefined||clickElement==null){$(element).hide();}else{newOverlayElements.push(element);}}else{$(element).hide();}}else{$(element).hide();}}}
var newOverlayElements=new Array;var activeOverlayElements=new Array;var processBodyClick=function(event){var clickedElement=event.element();isAButton=true;processClick=true;if(clickedElement==undefined||!clickedElement.hasClassName('GB_dropDownButton')){isAButton=false;containingMenu=clickedElement.up('.GB_dropDownArea_inner');if(containingMenu!=undefined&&containingMenu.hasClassName('GB_dropDownArea_persist')){processClick=false;}}
