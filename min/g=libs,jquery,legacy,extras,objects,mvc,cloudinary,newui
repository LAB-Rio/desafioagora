var tagAjaxCall=null;this.createNewTagEvents=function(containerElement){containerElement=$(containerElement);if(containerElement!=null){element=containerElement.select('.newItemTag')[0];if(element!=null){element.observe('keypress',function(event){Post.newtagFieldKeyPress(event,element);});element.observe('keydown',function(event){Post.newtagFieldKeyDown(event,element);});element.observe('keyup',function(event){Post.newTagFieldUpdate(event,element);});element.observe('blur',function(event){Post.newTagFieldBlur(event,element);});}}}
this.newTagFieldUpdate=function(event,element){var contents=$F(element);var newTag;var commaPosition=contents.indexOf(',');var tagFormValue;var KeyID=event.keyCode;var postTags;if(tagAjaxCall!=null){tagAjaxCall.transport.abort();}
if((commaPosition!=-1)||((KeyID==13)&&(contents.length!=0))){if(KeyID==13){commaPosition=contents.length;}
Post.addTagToField(contents,commaPosition,element);event.stop();}else if((contents.length>3)&&(KeyID!=8)&&(KeyID!=46)){tagAjaxCall=new Ajax.Request('/tag/similartag/?term='+contents,{onComplete:function(response){if(response.responseText.length>contents.length){if(response.responseText.substr(0,$F(element).length)==$F(element)){element.setValue(response.responseText);createSelection(element,contents.length,response.responseText.length);}}}});}
if(KeyID==13){event.stop();}}
this.newTagFieldBlur=function(event,element){var contents=$F(element);var commaPosition=contents.length;Post.addTagToField(contents,commaPosition,element);}
this.newTagFieldFocus=function(event,element){element.focus();}
this.newtagFieldKeyPress=function(event,element){var KeyID=event.keyCode;if(KeyID==13){event.stop();}}
this.newtagFieldKeyDown=function(event,element){var KeyID=event.keyCode;var contents=$F(element);if((KeyID==8)&&(contents.length==0)){postTags=$$('#postForm_tagArea .itemTag');if(postTags.length>0){Post.removeTagFromTagField(postTags[postTags.length-1]);}}else if(KeyID==13){event.stop();}}
this.addTagToField=function(contents,commaPosition,element){var newTag=contents.substr(0,commaPosition);if(newTag.length>1){var newTagElement=new Element('div',{'class':'itemTag'});newTag=newTag.replace(/[\u00A0-\u99999<>\&]/gim,function(i){return'&#'+i.charCodeAt(0)+';';});newTagElement.update('<span>'+newTag+'</span>'+'<a href="#" onclick="Post.removeTagFromTagField($(this).up()); return false;">x</a>');$(element).insert({before:newTagElement});var tagFormValue=$F('postForm_tags');tagFormValue+=","+newTag;$('postForm_tags').setValue(tagFormValue);}
$(element).setValue(contents.substr(commaPosition+1));}
this.removeTagFromTagField=function(tagElement){tagFormValue=$F('postForm_tags');tagFormValue=tagFormValue.replace(tagElement.down('span').innerHTML,'');$('postForm_tags').setValue(tagFormValue);tagElement.remove();}
